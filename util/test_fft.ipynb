   "source": [
    "import torch\n",
    "\n",
    "# 假设 input shape 是 [b=2, a=3, c=4, t=8]\n",
    "x = torch.rand(2, 3, 4, 8)\n",
    "\n",
    "# 对最后一个维度 t=8 执行 rFFT\n",
    "y = torch.fft.fft(x, dim=-1)\n",
    "\n",
    "print(\"Input shape:\", x.shape)\n",
    "print(\"Output shape:\", y.shape)"
   ]
